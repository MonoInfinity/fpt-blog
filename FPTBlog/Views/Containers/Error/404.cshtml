@{
    ViewData["Title"] = "Not Found";
}

<div class="flex flex-col flex-1 pt-16 bg-white">
    <main class="flex flex-col justify-center flex-grow w-full px-4 mx-auto max-w-7xl sm:px-6 lg:px-8">
        <div class="py-16">
            <div class="text-center">
                <p class="text-sm font-semibold tracking-wide uppercase text-tango-600">404 error</p>
                <h1 class="mt-2 text-4xl font-extrabold tracking-tight text-gray-900 sm:text-5xl">Page not found.</h1>
                <p class="mt-2 text-base text-gray-500">Sorry, we couldn’t find the page you’re looking for.</p>
                <div class="mt-6">
                    <a href="@Routers.CommonGetHome.Link"
                        class="text-base font-medium text-tango-600 hover:text-tango-500">Go back home<span
                            aria-hidden="true"> &rarr;</span></a>
                </div>
            </div>
        </div>
    </main>
</div>
